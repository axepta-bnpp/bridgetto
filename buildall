echo
echo Building smallbridge
echo ------------------------------------
echo building macosx
go build -o dist/amd64/macosx/smallbridge src/bridge.go
echo building windows
GOOS=windows go build -o dist/amd64/windows/smallbridge.exe src/bridge.go
echo building linux
GOOS=linux go build -o dist/amd64/linux/smallbridge src/bridge.go
echo